<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTintMode="add"
    android:backgroundTint="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".counter.MapRunnerFragment">
    <ImageView
        android:id="@+id/imageview"
        android:layout_width="45dp"
        android:layout_height="30dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="67dp"
        android:src="@drawable/menuicon"
        android:layout_marginLeft="15sp"
        android:layout_marginRight="15sp"
        app:layout_constraintEnd_toStartOf="@id/textview"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:text="MAP RUNNER"
        android:fontFamily="sans-serif-condensed-medium"
        android:textSize="22dp"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:id="@+id/giftbox"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:layout_marginTop="18dp"
        android:layout_marginLeft="60sp"
        android:layout_marginRight="30sp"
        android:src="@drawable/https___lottiefiles_com_animations_gift_box_animation_uyfmxovjc7"
        app:layout_constraintStart_toEndOf="@id/textview"
        app:layout_constraintTop_toTopOf="parent"
        />
    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:background="#C6C4C4"
        android:layout_height="2dp"
        android:visibility="visible"
        android:layout_marginTop="20sp"
        app:layout_constraintTop_toBottomOf="@id/imageview"
        />

    <FrameLayout
        android:id="@+id/framelayout1"
        android:layout_width="100dp"
        android:layout_height="121dp"
        android:layout_marginStart="14sp"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="10sp"
        android:background="@drawable/view_bg"
        app:layout_constraintStart_toStartOf="@id/view"
        app:layout_constraintTop_toBottomOf="@id/view">

        <ImageView
            android:id="@+id/imageviewfire"
            android:layout_width="52dp"
            android:layout_height="42dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="8sp"
            android:paddingLeft="4sp"
            android:scaleType="fitStart"
            android:src="@drawable/fire_icon"></ImageView>

        <TextView
            android:layout_width="91dp"
            android:layout_height="51dp"
            android:layout_marginHorizontal="15sp"
            android:layout_marginTop="65sp"
            android:paddingLeft="0sp"
            android:text="Calories 0.0 Kcal"
            android:textSize="18dp"
            android:textStyle="bold" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/framelayout2"
        android:layout_width="100dp"
        android:layout_height="121dp"
        android:layout_marginStart="10sp"
        android:layout_marginLeft="10sp"
        android:layout_marginTop="40dp"
        android:background="@drawable/view_bg"
        app:layout_constraintStart_toEndOf="@id/framelayout1"
        app:layout_constraintTop_toBottomOf="@id/view">

        <ImageView
            android:id="@+id/imageviewdistance"
            android:layout_width="52dp"
            android:layout_height="42dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="8sp"
            android:paddingLeft="4sp"
            android:scaleType="fitStart"
            android:src="@drawable/distance_icon"></ImageView>

        <TextView
            android:layout_width="91dp"
            android:layout_height="51dp"
            android:layout_marginHorizontal="15sp"
            android:layout_marginTop="65sp"
            android:paddingLeft="0sp"
            android:text="Distance   0 Mile"
            android:textSize="18dp"
            android:textStyle="bold" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/framelayout3"
        android:layout_width="100dp"
        android:layout_height="119dp"
        android:layout_marginStart="10sp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="0sp"
        android:layout_marginLeft="10sp"
        android:background="@drawable/view_bg"
        app:layout_constraintStart_toEndOf="@id/framelayout2"
        app:layout_constraintTop_toBottomOf="@id/view">

        <ImageView
            android:id="@+id/imageviewduration"
            android:layout_width="52dp"
            android:layout_height="42dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="8sp"
            android:paddingLeft="4sp"
            android:scaleType="fitStart"
            android:src="@drawable/duration_icon"></ImageView>

        <TextView
            android:layout_width="91dp"
            android:layout_height="51dp"
            android:layout_marginHorizontal="15sp"
            android:layout_marginTop="65sp"
            android:paddingLeft="0sp"
            android:text="Calories 0.0 Kcal"
            android:textSize="18dp"
            android:textStyle="bold" />

    </FrameLayout>

    <TextView
        android:id="@+id/timmer"
        android:layout_width="127dp"
        android:layout_height="38dp"
        android:layout_marginTop="28dp"
        android:gravity="center_horizontal"
        android:paddingTop="5sp"
        android:text="00:00:00"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@id/framelayout2"
        app:layout_constraintTop_toBottomOf="@id/framelayout2" />

    <Button
        android:id="@+id/b1"
        android:layout_width="150dp"
        android:layout_height="43dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/background_button"
        android:text="Start"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.551"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/timmer" />



</androidx.constraintlayout.widget.ConstraintLayout>